    "s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)\n",
    "s.connect((socket.gethostname(), 9620))\n",
    "data = \"hello\"\n",
    "s.send(data.encode())\n",
    "msg = s.recv(1024)\n",
    "print(msg.decode(\"utf-8\"))\n",
    "s.close()"
